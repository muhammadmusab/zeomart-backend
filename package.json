{
  "name": "@codelab/minimal-ecommerce",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "serve": "concurrently \"npx tsc -w\"  \"nodemon dist/index.js\"",
    "migrate:create": "node src/migrate.js create --name",
    "migrate:create:prod": "dotenv -e .env.production -- node src/migrate.js create --name",
    "migrate:create:dev": "dotenv -e .env.local -- node src/migrate.js create --name",
    "migrate:up:prod": "dotenv -e .env.production -- node src/migrate.js up",
    "migrate:up": "node src/migrate.js up",
    "migrate:up:dev": "dotenv -e .env.local -- node src/migrate.js up",
    "migrate:down:prod": "dotenv -e .env.production -- node src/migrate.js down",
    "migrate:down": "node src/migrate.js down",
    "migrate:down:dev": "dotenv -e .env.local -- node src/migrate.js down",
    "migrate-up-name": "node src/migrate.js up --name",
    "migrate-up-name:dev": "dotenv -e .env.local -- node src/migrate.js up --name",
    "migrate-up-name:prod": "dotenv -e .env.production -- node src/migrate.js up --name",
    "migrate-down-name": "node src/migrate.js down --name",
    "migrate-down-name:dev": "dotenv -e .env.local -- node src/migrate.js down --name",
    "migrate-down-name:prod": "dotenv -e .env.production -- node src/migrate.js down --name",
    "migrate-reset": "node src/migrate.js down --to 0",
    "migrate-reset:prod": "dotenv -e .env.production -- node src/migrate.js down --to 0",
    "migrate-reset:dev": "dotenv -e .env.local -- node src/migrate.js down --to 0",
    "seed:create:dev": "dotenv -e .env.local -- node src/seed.js create --name",
    "seed:up:dev": "dotenv -e .env.local -- node src/seed.js up",
    "seed:down:dev": "dotenv -e .env.local -- node src/seed.js down",
    "seed-up-name:dev": "dotenv -e .env.local -- node src/seed.js up --name",
    "seed-down-name:dev": "dotenv -e .env.local -- node src/seed.js down --name",
    "seed-reset:dev": "dotenv -e .env.local -- node src/seed.js down --to 0",
    "start": "node dist/index.js",
    "start:dev": "dotenv -e .env.local -- ts-node-dev --poll --files src/index.ts",
    "start:prod": "dotenv -e .env.production nodemon dist/index.js",
    "start:local": "dotenv -e .env.local nodemon dist/index.js ",
    "start-nodemon:dev": "dotenv -e .env.local -- nodemon dist/index.js",
    "start-nodemon:prod": "dotenv -e .env.production -- nodemon dist/index.js",
    "build": "rimraf ./dist && tsc -p . && copyfiles -u 1 src/**/*.hbs dist/ && copyfiles -u 1 src/**/*.pem dist/",
    "test": "jest --watchAll --no-cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mailchimp__mailchimp_marketing": "^3.0.6",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/nodemailer-sendgrid": "^1.0.0",
    "@types/uuid": "^9.0.8",
    "aws-sdk": "^2.1328.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^6.11.2",
    "google-auth-library": "^8.7.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.0.0",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "order-id": "^2.1.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qs": "^6.13.0",
    "rimraf": "^4.1.1",
    "sequelize": "^6.37.1",
    "sharp": "^0.31.3",
    "stripe": "^17.3.1",
    "ts-node-dev": "^2.0.0",
    "umzug": "^3.2.1",
    "uuid": "^9.0.1",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.32.0",
    "nodemon": "^2.0.22",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^4.9.4"
  }
}
